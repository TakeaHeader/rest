<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rest.mapper.AuthMapper">
    <!-- 查询商品条码 -->
    <select id="getUser" parameterType="java.util.Map" resultType="java.util.Map">
      SELECT A.ID ,A.USERNAME FROM SYS_USER A WHERE A.USERNAME = #{username} AND A.PASSWORD = #{password}
    </select>

</mapper>
